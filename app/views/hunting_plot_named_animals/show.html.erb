<div class="row">
  <div class="col-md-12">

    <p>
      <strong>Hunting plot:</strong>
      <%= (@hunting_plot_named_animal.hunting_plot_id.nil?) ? nil : @hunting_plot_named_animal.hunting_plot.get_display_name %>
    </p>

    <p>
      <strong>Name:</strong>
      <%= @hunting_plot_named_animal.name %>
    </p>

    <p>
      <strong>Animal category:</strong>
      <%= (@hunting_plot_named_animal.animal_category_id.nil?) ? nil : @hunting_plot_named_animal.animal_category.get_display_name %>
    </p>

    <%= link_to 'Edit', edit_hunting_plot_named_animal_path(@hunting_plot_named_animal) %> |
    <%= link_to 'Back', hunting_plot_named_animals_path %>

  </div>
</div>
