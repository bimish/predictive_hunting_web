<%= bootstrap_form_for(@animal_activity_observation) do |f| %>
  <% if @animal_activity_observation.errors.any? %>
    <%= render :partial => '/shared/activerecord_errors', :locals => { :active_record => @animal_activity_observation } %>
  <% end %>
  <%= render :partial => 'form_fields', :locals => { :form => f } %>
  <%= _submit_button(f, @animal_activity_observation) %>
<% end %>
