<div class="row">
  <div class="col-md-12">

    <p>
      <strong>Name:</strong>
      <%= @user_group.name %>
    </p>
    <p>
      <strong>Group type:</strong>
      <%= EnumsHelper::UserGroupEnums::get_group_type_description(@user_group.group_type) %>
    </p>
    <p>
      <strong>Hunting plot:</strong>
      <%= (@user_group.hunting_plot_id.nil?) ? nil : @user_group.hunting_plot.get_display_name %>
    </p>

    <%= link_to 'Edit', edit_user_group_path(@user_group) %> |
    <%= link_to 'Back', user_groups_path %>

  </div>
</div>
