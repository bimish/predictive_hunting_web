<% provide(:title, 'User Network Boundary') %>
<%= bootstrap_form_for @user_network_boundary, remote: true do |f| %>
  <% if @user_network_boundary.errors.any? %>
    <%= render :partial => '/shared/activerecord_errors', :locals => { :active_record => @user_network_boundary } %>
  <% end %>
  <%= render :partial => 'form_fields', :locals => { :form => f } %>
<% end %>
