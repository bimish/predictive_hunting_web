<%= bootstrap_form_for(@user_network_category) do |f| %>
  <% if @user_network_category.errors.any? %>
    <%= render :partial => '/shared/activerecord_errors', :locals => { :active_record => @user_network_category } %>
  <% end %>
  <%= render :partial => 'form_fields', :locals => { :form => f } %>
  <%= _submit_button(f, @user_network_category, 'User Network Category') %>
<% end %>
