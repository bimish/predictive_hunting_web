<tr data-id="<%=user_network.id%>">
  <td><%= user_network.name %></td>
  <td><%= (user_network.category_id.nil?) ? nil : user_network.category.get_display_name %></td>
  <td><%= (user_network.parent_network_id.nil?) ? nil : user_network.parent_network.get_display_name %></td>
  <td><%= item_show_command(user_network_path(user_network)) %></td>
  <td><%= item_edit_command(edit_user_network_path(user_network), { remote: true }) %></td>
  <td><%= item_delete_command(user_network_path(user_network), { remote: true, method: :delete, data: { confirm: 'Are you sure?' } }) %></td>
</tr>
