<%= bootstrap_form_for([@user_network,@composite_network_member]) do |f| %>
  <% if @composite_network_member.errors.any? %>
    <%= render :partial => '/shared/activerecord_errors', :locals => { :active_record => @composite_network_member } %>
  <% end %>
  <%= render :partial => 'form_fields', :locals => { :form => f } %>
  <%= _submit_button(f, @composite_network_member) %>
<% end %>
