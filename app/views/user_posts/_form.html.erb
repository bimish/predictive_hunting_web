<%= bootstrap_form_for(@user_post) do |f| %>
  <% if @user_post.errors.any? %>
    <%= render :partial => '/shared/activerecord_errors', :locals => { :active_record => @user_post } %>
  <% end %>
  <%= render :partial => 'form_fields', :locals => { :form => f } %>
  <%= _submit_button(f, @user_post, 'User Post') %>
<% end %>
