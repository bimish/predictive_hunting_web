<% provide(:title, 'Hunting Plot User Access') %>
<%= bootstrap_form_for @hunting_plot_user_access, remote: true do |f| %>
  <% if @hunting_plot_user_access.errors.any? %>
    <%= render :partial => '/shared/activerecord_errors', :locals => { :active_record => @hunting_plot_user_access } %>
  <% end %>
  <%= render :partial => 'form_fields', :locals => { :form => f } %>
<% end %>
