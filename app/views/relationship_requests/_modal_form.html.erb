<% provide(:title, 'Relationship Request') %>
<%= bootstrap_form_for @relationship_request, remote: true do |f| %>
  <% if @relationship_request.errors.any? %>
    <%= render :partial => '/shared/activerecord_errors', :locals => { :active_record => @relationship_request } %>
  <% end %>
  <%= render :partial => 'form_fields', :locals => { :form => f } %>
<% end %>
