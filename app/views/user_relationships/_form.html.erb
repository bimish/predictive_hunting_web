<%= bootstrap_form_for(@user_relationship) do |f| %>
  <% if @user_relationship.errors.any? %>
    <%= render :partial => '/shared/activerecord_errors', :locals => { :active_record => @user_relationship } %>
  <% end %>
  <%= render :partial => 'form_fields', :locals => { :form => f } %>
  <%= _submit_button(f, @user_relationship, 'User Relationship') %>
<% end %>
